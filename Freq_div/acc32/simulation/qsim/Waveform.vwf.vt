`timescale 1 ps/ 1 ps
module acc32_vlg_sample_tst(
	clk,
	en,
	rst,
	sampler_tx
);
input  clk;
input  en;
input  rst;
output sampler_tx;

reg sample;
time current_time;
always @(clk or en or rst)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

