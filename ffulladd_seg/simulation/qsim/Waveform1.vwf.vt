end 

ffulladd_seg_vlg_sample_tst tb_sample (
	.a(a),
	.b(b),
	.m(m),
	.sampler_tx(sampler)
);

ffulladd_seg_vlg_check_tst tb_out(
	.cout(cout),
	.s(s),
	.v(v),
	.sampler_rx(sampler)
