reg [3:0] b;
reg m;
// wires                                               
wire cout;
wire [3:0] s;
wire v;

wire sampler;                             

// assign statements (if any)                          
ffulladd_seg i1 (
// port map - connection between master ports and signals/registers   
	.a(a),
	.b(b),
	.cout(cout),
	.m(m),
	.s(s),
	.v(v)
);
// a[ 3 ]
initial
begin
	a[3] = 1'b0;
	a[3] = #10000 1'b1;
	a[3] = #10000 1'b0;
end 
// a[ 2 ]
